<div class="row sub-header">
    <h3>Products</h3>
</div>
<div class="row">
    <button  class="btn btn-info pull-left clsManageProduct" object-id="-1" object-mode="A">
        <span class="glyphicon glyphicon-plus"></span> Add New Product
    </button>
    <button class="btn btn-info pull-right clsManageCategories">
        <span class="glyphicon glyphicon-pencil"></span> Manage Categories
    </button>
</div>

<div class="row dv-upper-margin">
    <div class="col-md-12 input-group">
        <input type="text" class="form-control search-box clsProductSearch" placeholder="Search Product">

    </div>
</div>
<div class="row dv-upper-margin">
    <button class="btn btn-info pull-right clsRefreshProducts">
        <span class="glyphicon glyphicon-refresh"></span>
    </button>
</div>
<div class="row">
    <div class="dv-product-details clsProductDetails">
        @Html.Action("_ProductDetails")
    </div>
</div>

